<?xml version="1.0" encoding="iso-8859-1"?>
<xs:schema id="Products" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop">
	<xs:element name="Products" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">		
				<xs:element name="Product">
         			<xs:complexType>
         				<xs:sequence>
         					<xs:element msprop:TableNameXsd="Products" msprop:IsIdentity="true" name="sku" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="0" maxOccurs="1"/>
         			        <xs:element  name="Photos_Cloned_From"  msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="0" maxOccurs="255"/>
							<xs:element name="price"   msprop:SqlDbType="Money" type="xs:decimal" minOccurs="0" maxOccurs="1" />
						
								 
							<xs:element name="name" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="1" maxOccurs="1"/>
							<xs:element name="description"   msprop:maxLength="255" msprop:SqlDbType="Text" type="xs:string" minOccurs="0" maxOccurs="1"/> 
							<xs:element name="thumbnail_image_URL" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="1" maxOccurs="1"/>
		                    <xs:element name="Large_Image_URL" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="1" maxOccurs="1"/>
							<xs:element name="detail_page_URL"  msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="1" maxOccurs="1"/>
							<xs:element name="retail_price"   msprop:SqlDbType="Money"  minOccurs="0" maxOccurs="1" type="xs:decimal" />
							
						 
					        <xs:element name="Short_Product_Description" msprop:maxLength="255"  type="xs:string" minOccurs="0" maxOccurs="1"/>
							
							
							
							
			    		</xs:sequence>  
					</xs:complexType>	
				</xs:element>
			</xs:choice>
		</xs:complexType>		
	</xs:element>
</xs:schema>